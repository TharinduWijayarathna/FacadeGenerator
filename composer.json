{
    "name": "tharindu/facade_generator",
    "description": "A Laravel package to generate facades and services.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Tharindu\\FacadeGenerator\\": "src/",
            "Domain\\": "domain/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tharindu\\FacadeGenerator\\FacadeGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
